# parameter
use graph testGraph
CREATE or replace DISTRIBUTED QUERY getAttr_no_target_dist(string vType,String attrName) FOR GRAPH testGraph{
  Start={vType};
  result= select s from Start:s-(:e)- :t
          ACCUM s.getAttr(attrName,"string"),
                e.getAttr(attrName,"string")
                ;
}

install query getAttr_no_target_dist